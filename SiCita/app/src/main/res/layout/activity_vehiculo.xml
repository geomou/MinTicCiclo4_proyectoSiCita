<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Vehiculo">

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="41dp"
        android:layout_height="35dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="14dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/nombre19"
        app:layout_constraintTop_toBottomOf="@+id/imageView20"
        app:srcCompat="@android:drawable/ic_search_category_default" />

    <EditText
        android:id="@+id/nombre19"
        android:layout_width="250dp"
        android:layout_height="35dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="40dp"
        android:ems="10"
        android:hint="@string/Nombre"
        android:inputType="textPersonName"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/imageButton14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView22" />

    <ImageView
        android:id="@+id/imageView20"
        android:layout_width="115dp"
        android:layout_height="40dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="2dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_car" />

    <ImageView
        android:id="@+id/imageView22"
        android:layout_width="57dp"
        android:layout_height="46dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="235dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView20"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_rojo_blanco" />

    <LinearLayout
        android:id="@+id/linearLayout3"
        android:layout_width="350dp"
        android:layout_height="223dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        android:orientation="vertical"
        android:textAlignment="textEnd"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nombre19">

        <TextView
            android:id="@+id/textView3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            tools:text="@string/placa" />

        <Spinner
            android:id="@+id/spinner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Spinner
            android:id="@+id/spinner4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content" />

        <Spinner
            android:id="@+id/spinner5"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:entries="@array/añosmodelo"
            android:textAlignment="center" />

        <EditText
            android:id="@+id/editTextTextPersonName"
            android:layout_width="match_parent"
            android:layout_height="87dp"
            android:ems="10"
            android:hint="@string/detalleveh"
            android:inputType="textPersonName" />

        <Button
            android:id="@+id/button14"
            android:layout_width="128dp"
            android:layout_height="42dp"
            android:text="@string/actualizar" />

    </LinearLayout>

    <ScrollView
        android:layout_width="350dp"
        android:layout_height="219dp"
        android:layout_marginStart="26dp"
        android:layout_marginEnd="26dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout3">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TableLayout
                android:id="@+id/tableLayout3"
                android:layout_width="323dp"
                android:layout_height="139dp">

                <TableRow
                    android:layout_width="421dp"
                    android:layout_height="224dp"
                    android:scrollbars="vertical"
                    android:showDividers="middle|beginning|end|none">


                    <TextView
                        android:layout_width="61dp"
                        android:layout_height="wrap_content"
                        android:background="#b0b0b0"
                        android:padding="8dip"
                        android:text="@string/placa"
                        android:textColor="#000"
                        android:textSize="12dp" />


                    <TextView
                        android:layout_width="96dp"
                        android:layout_height="wrap_content"
                        android:background="#b0b0b0"
                        android:padding="8dip"
                        android:text="@string/marca"
                        android:textColor="#000"
                        android:textSize="12dp" />

                    <TextView
                        android:layout_width="125dp"
                        android:layout_height="wrap_content"
                        android:background="#b0b0b0"
                        android:padding="8dip"
                        android:text="@string/modelo"
                        android:textColor="#000"
                        android:textSize="12dp" />

                    <TextView
                        android:layout_width="61dp"
                        android:layout_height="wrap_content"
                        android:background="#b0b0b0"
                        android:padding="8dip"
                        android:text="@string/añoveh"
                        android:textColor="#000"
                        android:textSize="12dp" />

                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="14dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="14dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="14dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="14dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />
                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="14dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="14dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="14dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="14dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />


                </TableRow>

                <TableRow
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="18dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />


                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="18dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="18dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:padding="14dip"
                        android:text=""
                        android:textColor="#000"
                        android:textSize="12dp" />

                </TableRow>
            </TableLayout>
        </LinearLayout>
    </ScrollView>

</androidx.constraintlayout.widget.ConstraintLayout>