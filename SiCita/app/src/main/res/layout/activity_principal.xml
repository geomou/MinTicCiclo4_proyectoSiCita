<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.Principal">

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="115dp"
        android:layout_height="124dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="8dp"
        android:adjustViewBounds="true"
        android:onClick="goToActivity2"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imbsede"
        app:srcCompat="@drawable/logo_tecnico" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="115dp"
        android:layout_height="124dp"
        android:layout_marginTop="8dp"
        android:adjustViewBounds="true"
        android:onClick="goToActivity3"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toEndOf="@+id/imageButton9"
        app:layout_constraintTop_toBottomOf="@+id/imbsede"
        app:srcCompat="@drawable/logo_sede" />

    <ImageButton
        android:id="@+id/imageButton11"
        android:layout_width="115dp"
        android:layout_height="124dp"
        android:layout_marginStart="68dp"
        android:layout_marginTop="8dp"
        android:adjustViewBounds="true"
        android:onClick="goToActivity4"
        android:scaleType="centerInside"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton9"
        app:srcCompat="@drawable/logo_usuario" />

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="115dp"
        android:layout_height="124dp"
        android:layout_marginTop="8dp"
        android:adjustViewBounds="true"
        android:onClick="goToActivity5"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.318"
        app:layout_constraintStart_toEndOf="@+id/imageButton11"
        app:layout_constraintTop_toBottomOf="@+id/imageButton10"
        app:srcCompat="@drawable/reporte_rojo" />

    <ImageButton
        android:id="@+id/imbEmpresa"
        android:layout_width="186dp"
        android:layout_height="77dp"
        android:layout_marginTop="16dp"
        android:adjustViewBounds="true"
        android:onClick="goToActivity"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"

        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_carempresa" />

    <ImageButton
        android:id="@+id/imbsede"
        android:layout_width="186dp"
        android:layout_height="77dp"
        android:layout_marginTop="12dp"
        android:adjustViewBounds="true"
        android:onClick="goToActivity1"
        android:scaleType="centerInside"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imbEmpresa"
        app:srcCompat="@drawable/logo_car" />
</androidx.constraintlayout.widget.ConstraintLayout>