        android:layout_height="47dp"
        android:layout_marginStart="39dp"
        android:layout_marginTop="4dp"
        android:layout_marginEnd="40dp"
        android2:layout_height="match_parent"
        android2:layout_width="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spnempresa"
        tools2:context=".MainActivity">

        <EditText
            android:id="@+id/in_date"
            android:layout_width="97dp"
            android:layout_height="36dp"
            android:layout_alignParentStart="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentTop="true"
            android:layout_marginStart="2dp"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="11dp"
            android:textSize="10sp" />

        <Button
            android:id="@+id/btn_date"
            android:layout_width="75dp"
            android:layout_height="35dp"
            android:layout_alignBottom="@+id/in_date"
            android:layout_alignParentTop="true"
            android:layout_marginStart="4dp"
            android:layout_marginLeft="4dp"
            android:layout_marginTop="-4dp"
            android:layout_marginBottom="10dp"
            android:layout_toEndOf="@+id/in_date"
            android:layout_toRightOf="@+id/in_date"
            android:text="@string/fecha"
            android:textSize="10sp" />

        <EditText
            android:id="@+id/in_time"
            android:layout_width="69dp"
            android:layout_height="32dp"
            android:layout_below="@+id/in_date"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="194dp"
            android:layout_marginTop="-32dp"
            android:textSize="10sp" />

        <Button
            android:id="@+id/btn_time"
            android:layout_width="77dp"
            android:layout_height="40dp"
            android:layout_below="@+id/btn_date"
            android:layout_alignLeft="@+id/btn_date"
            android:layout_marginLeft="163dp"
            android:layout_marginTop="-45dp"
            android:text="@string/hora"
            android:textSize="10sp" />

    </RelativeLayout>

    <CheckBox
        android:id="@+id/checkBox13"
        android:layout_width="148dp"
        android:layout_height="20dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="2dp"
        android:text="@string/buscarfecha"
        android:theme="@style/fontssicita"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout" />

    <CheckBox
        android:id="@+id/checkBox14"
        android:layout_width="159dp"
        android:layout_height="20dp"
        android:layout_marginStart="9dp"
        android:layout_marginTop="2dp"
        android:text="@string/buscarsede"
        android:theme="@style/fontssicita"
        app:layout_constraintStart_toEndOf="@+id/checkBox13"
        app:layout_constraintTop_toBottomOf="@+id/relativeLayout" />

