<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:android2="https://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:tools2="https://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CitaBuscar">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="44dp"
        android:layout_height="18dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="1dp"
        android:text="@string/placa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <Spinner
        android:id="@+id/idsnpnplaca"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginStart="85dp"
        android:layout_marginTop="60dp"
        android:layout_marginEnd="86dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/idbtncrearcita"
        android:layout_width="128dp"
        android:layout_height="42dp"
        android:layout_marginStart="244dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="39dp"
        android:onClick="Actualizar"
        android:text="@string/crearcita"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idtxvidsedebuscar" />

    <Button
        android:id="@+id/idbtnbuscarcita"
        android:layout_width="68dp"
        android:layout_height="29dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="25dp"
        android:onClick="Buscar"
        android:text="@string/CitaBuscar"
        android:textSize="8sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />

    <TextView
        android:id="@+id/idtxvplaca"
        android:layout_width="53dp"
        android:layout_height="18dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="1dp"
        android:text="@string/placa"
        app:layout_constraintStart_toEndOf="@+id/textView3"
        app:layout_constraintTop_toBottomOf="@+id/idtxvidsedebuscar" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="44dp"
        android:layout_height="18dp"
        android:layout_marginStart="48dp"
        android:text="@string/Sede"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView12" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="44dp"
        android:layout_height="18dp"
        android:layout_marginStart="48dp"
        android:text="@string/hora"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="44dp"
        android:layout_height="18dp"
        android:layout_marginStart="48dp"
        android:text="@string/fecha"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idlvresultadobuscar" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="4dp"
        android:text="@string/Sede"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <Spinner
        android:id="@+id/idsnpsede"
        android:layout_width="281dp"
        android:layout_height="26dp"
        android:layout_marginStart="3dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/textView10"
        app:layout_constraintTop_toBottomOf="@+id/idspnempresa" />

    <ImageView
        android:id="@+id/imageView25"
        android:layout_width="64dp"
        android:layout_height="60dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_base_car" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="56dp"
        android:layout_marginTop="6dp"
        android:text="@string/Empresa"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idchkbuscar" />

    <Spinner
        android:id="@+id/idspnempresa"
        android:layout_width="240dp"
        android:layout_height="25dp"
        android:layout_marginTop="1dp"
        android:layout_marginEnd="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idchkbuscar" />

    <CheckBox
        android:id="@+id/idchkbuscar"
        android:layout_width="114dp"
        android:layout_height="20dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="4dp"
        android:text="@string/buscarfecha"
        android:theme="@style/fontssicita"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />

    <CheckBox
        android:id="@+id/idchkbuscarsede"
        android:layout_width="110dp"
        android:layout_height="19dp"
        android:layout_marginStart="2dp"
        android:layout_marginTop="4dp"
        android:text="@string/buscarsede"
        android:theme="@style/fontssicita"
        app:layout_constraintStart_toEndOf="@+id/idchkbuscar"
        app:layout_constraintTop_toBottomOf="@+id/linearLayout4" />

    <TextView
        android:id="@+id/idtxvfecha"
        android:layout_width="81dp"
        android:layout_height="18dp"
        android:layout_marginStart="8dp"
        android:hint="@string/fecha"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/idlvresultadobuscar" />

    <TextView
        android:id="@+id/idtxvhora"
        android:layout_width="81dp"
        android:layout_height="18dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="1dp"
        android:hint="@string/hora"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/idtxvfecha" />

    <TextView
        android:id="@+id/idtxvidsedebuscar"
        android:layout_width="200dp"
        android:layout_height="18dp"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="112dp"
        android:hint="@string/Sede"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView13"
        app:layout_constraintTop_toBottomOf="@+id/idtxvhora" />

    <ListView
        android:id="@+id/idlvresultadobuscar"
        android:layout_width="329dp"
        android:layout_height="145dp"
        android:layout_marginStart="48dp"
        android:layout_marginEnd="34dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idsnpsede" />

    <LinearLayout
        android:id="@+id/linearLayout4"
        android:layout_width="270dp"
        android:layout_height="38dp"
        android:layout_marginStart="64dp"
        android:layout_marginEnd="59dp"
        android:orientation="horizontal"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/idsnpnplaca">

        <EditText
            android:id="@+id/in_date"
            android:layout_width="60dp"
            android:layout_height="36dp"
            android:textSize="10sp" />

        <Button
            android:id="@+id/btn_date"
            android:layout_width="72dp"
            android:layout_height="29dp"
            android:text="@string/fecha"
            android:textSize="8sp" />

        <EditText
            android:id="@+id/in_time"
            android:layout_width="50dp"
            android:layout_height="32dp"
            android:textSize="10sp" />

        <Button
            android:id="@+id/btn_time"
            android:layout_width="77dp"
            android:layout_height="29dp"
            android:text="@string/hora"
            android:textSize="8sp" />
    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
